public class Interview {
    // Private attributes
    private int interviewID;      // Unique identifier for the interview
    private int candidateID;      // Unique identifier for the candidate
    private int interviewerID;     // Unique identifier for the interviewer
    private int scheduleDate;      // Scheduled date for the interview
    private String feedback;       // Feedback provided after the interview

    // Method to record feedback
    public void recordFeedback(String feedback) {
        this.feedback = feedback;  // Assign feedback to the feedback attribute
    }
}
